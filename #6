#include <iostream>
#include <string>
using namespace std;

int main()
{
  cout << "Fibonacci numbers:" << endl;
  int ctr = 0;
  int temp1 = 0, temp2 = 1, sum = 0;
  cout << temp1 << "," << temp2 << ",";
  while(ctr < 20)
  {
     sum = temp1 + temp2;
      cout << sum;
      if(ctr < 19)
        cout << ",";
      ctr++;
      
      temp1 = temp2;
      temp2 = sum;
      }
}
