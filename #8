#include <iostream>

using namespace std;

int main()
{
    int num;
    cout << "Enter a number: " << endl; 
    cin >> num;
    
    cout << "Multiplication table of: " << num << endl;
    for(int i = 1; i < 11; i++){
        
        cout << num << " * " << i << " = " << i * num << endl;
    }
    

   return 0;
}
